<p id="notice"><%= notice %></p>

<h1>Transmissions</h1>

<table class="table table-striped" style="background-color:#42F1BD;">
  <thead>
    <tr>
      <th>Date</th>
      <th>Pnumber</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transmissions.each do |transmission| %>
      <tr>
        <td><%= transmission.date %></td>
        <td><%= transmission.pnumber %></td>
        <td><%= link_to 'Show', transmission %></td>
        <td><%= link_to 'Edit', edit_transmission_path(transmission) %></td>
        <td><%= link_to 'Destroy', transmission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transmission', new_transmission_path %>
